@{
    ViewData["Title"] = "ChatGPT Explanation";
}
<h1>@ViewData["Title"]</h1>

<div id="insertHere">
</div>


<script>
    // Use Razor syntax to inject the backend value into JavaScript
    // This section was done before I was familiar with cshtml view models
    var backendMessage = "@ViewData["Message"]";

    const message = document.createElement("p");
    message.textContent = backendMessage;
    const target = document.getElementById("insertHere");
    target.appendChild(message);
</script>

